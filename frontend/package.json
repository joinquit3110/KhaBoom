{
  "name": "kha-boom-frontend",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "npx npm-run-all --sequential build:assets build:vite postbuild",
    "build:assets": "mgon-build --assets --locales en",
    "build:vite": "vite build",
    "postbuild": "node postbuild.js",
    "assets": "mgon-build --assets --locales en",
    "watch": "mgon-build --assets --watch --locales en",
    "start": "npx npm-run-all --parallel watch dev"
  },
  "dependencies": {
    "@mathigon/boost": "1.2.27",
    "@mathigon/core": "1.1.19",
    "@mathigon/euclid": "1.2.0",
    "@mathigon/fermat": "1.1.20",
    "@mathigon/hilbert": "1.1.20",
    "@mathigon/studio": "0.1.43",
    "axios": "^1",
    "framer-motion": "^10.16.4",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/d3": "6.3.0",
    "@types/three": "0.91.14",
    "@types/topojson": "3.2.3",
    "@vitejs/plugin-react": "^4.2.0",
    "npm-run-all": "4.1.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.39.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "vite": "^5",
    "vite-plugin-compression": "^0.5.1"
  }
